<view class="content"><view class="_caption"><view class="title">{{''+userName+', 您好'}}</view><view style="display:inline;"><image src="{{messageSrc}}" mode="{{scaleToFill}}" data-event-opts="{{[['tap',[['goMessage',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="check"><view>{{'签到情况: '+check_message+''}}</view><block wx:if="{{!isNotCheck}}"><view>{{'班次开始: '+start_at+''}}</view></block><block wx:if="{{!isNotCheck}}"><view style="padding-bottom:10rpx;">{{'班次结束: '+end_at+''}}</view></block></view><block wx:if="{{isNotCheck}}"><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['checkIn',['$event']]]]]}}" bindtap="__e">扫码</button></view></block><block wx:if="{{canCheckOff}}"><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['checkOff',['$event']]]]]}}" bindtap="__e">签退</button></view></block><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['goShiftArrangement',['$event']]]]]}}" bindtap="__e">查看班次安排</button></view><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['goLeave',['$event']]]]]}}" bindtap="__e">申请/查看请假</button></view><block wx:if="{{canICU}}"><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['goICU',['$event']]]]]}}" bindtap="__e">申请加班</button></view></block><view class="btn-row"><button class="primary" type="primary" data-event-opts="{{[['tap',[['goInfo',['$event']]]]]}}" bindtap="__e">查看个人信息</button></view></view>