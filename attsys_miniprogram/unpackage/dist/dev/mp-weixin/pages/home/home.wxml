<view class="content" bind:__l="__l"><view class="hello"><view class="title">{{'您好 '+userName+'，您已成功登录。'}}</view><view>{{'签到情况: '+check_message+''}}</view><view>{{'班次开始: '+start_at+''}}</view><view>{{'班次结束: '+end_at+''}}</view><view class="btn-row"><button class="primary" type="primary" data-account="{{account}}" data-token="token" data-event-opts="{{[['tap',[['scanQrCode',['$event']]]]]}}" bindtap="__e">扫码</button></view><view class="btn-row"><button class="primary" type="primary" data-account="{{account}}" data-token="token" data-event-opts="{{[['tap',[['goShiftArrangement',['$event']]]]]}}" bindtap="__e">查看班次安排</button></view><view class="btn-row"><button class="primary" type="primary" data-account="{{account}}" data-token="token" data-event-opts="{{[['tap',[['goLeave',['$event']]]]]}}" bindtap="__e">申请/查看请假</button></view><view class="btn-row"><button class="primary" type="primary" data-account="{{account}}" data-token="token" data-event-opts="{{[['tap',[['goICU',['$event']]]]]}}" bindtap="__e">申请加班</button></view><view class="btn-row"><button class="primary" type="primary" data-account="{{account}}" data-token="token" data-event-opts="{{[['tap',[['goInfo',['$event']]]]]}}" bindtap="__e">查看/修改信息</button></view></view></view>